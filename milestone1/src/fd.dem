set terminal tikz
set output "~/cs207-FinalProject/milestone1/fd.tex"
set xlabel "$x$"
set ylabel "$y$"
set grid
set key Left spacing 1.5 width 2.25
plot "fd_out.dat" using 1:2 with lines lw 4 title "$f'(x)$",\
